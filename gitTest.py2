#!/usr/bin/env python
# -*- coding: utf-8 -*-
import I2C_LCD_driver
import time
mylcd = I2C_LCD_driver.lcd()


while True:
    mylcd.lcd_display_string("Time: %s" %time.strftime("%H:%M:%S"), 1)
    
    mylcd.lcd_display_string("", 2)
    time.sleep(0.1)
